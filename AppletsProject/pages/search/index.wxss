page {
  background-color: #dedede;
  padding: 20rpx;
}
.search_row {
  height: 70rpx;
  display: flex;
}
.search_row input {
  background-color: white;
  flex: 3;
  height: 100%;
  padding-left: 30rpx;
}
.search_row button {
  flex: 1;
  display: flex;
  flex-direction: column;
  width: 30rpx !important;
  height: 100%;
  font-size: 28rpx;
  padding: 0;
  margin: 0 10rpx;
  justify-content: center;
  align-items: center;
  color: var(--themeColor);
}
.search_content {
  margin-top: 30rpx;
}
.search_content .search_item {
  background-color: #fff;
  font-size: 26rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1rpx solid #7ae5e9;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
