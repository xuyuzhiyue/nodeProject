page {
  height: 100%;
}
.cates {
  height: 100%;
}
.cates .cates_container {
  height: calc(100vh - 90rpx);
  display: flex;
}
.cates .cates_container .left_meun {
  flex: 2;
}
.cates .cates_container .left_meun .menu_item {
  height: 80rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.cates .cates_container .left_meun .active {
  color: var(--themeColor);
  border-left: 5px solid currentColor;
}
.cates .cates_container .rigth_content {
  flex: 5;
}
.cates .cates_container .rigth_content .goods_group .goods_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cates .cates_container .rigth_content .goods_group .goods_title .delimiter {
  color: #7dc5eb;
  padding: 0 10rpx;
}
.cates .cates_container .rigth_content .goods_group .goods_list {
  display: flex;
  flex-wrap: wrap;
}
.cates .cates_container .rigth_content .goods_group .goods_list navigator {
  width: 33.33%;
  text-align: center;
}
.cates .cates_container .rigth_content .goods_group .goods_list navigator image {
  width: 50%;
}
